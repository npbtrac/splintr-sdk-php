{
  "name": "splintr/php-sdk",
  "license": "MIT",
  "description": "Splintr PHP Client Library",
  "version": "0.0.1",
  "keywords": [
    "spinltr",
    "spinltr php sdk",
    "buy now pay later",
    "installment"
  ],
  "homepage": "https://www.splintr.com/",
  "authors": [
    {
      "name": "Splintr Team",
      "email": "tech@tamara.co"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-json": "*",
    "guzzlehttp/guzzle": "6.5.5"
  },
  "require-dev": {
    "coenjacobs/mozart": "~0.7.0",
    "symfony/var-dumper": "^3.0",
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Splintr\\PhpSdk\\": "src/"
    },
    "files": [
      "src/Dependencies/GuzzleHttp/functions_include.php",
      "src/Dependencies/GuzzleHttp/Promise/functions_include.php",
      "src/Dependencies/GuzzleHttp/Psr7/functions_include.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Splintr\\PhpSdk\\Tests\\": "tests/"
    }
  },
  "extra": {
    "mozart": {
      "dep_namespace": "Splintr\\PhpSdk\\Dependencies\\",
      "dep_directory": "/src/Dependencies/",
      "classmap_directory": "/classes/dependencies/",
      "classmap_prefix": "SplintrPhpSdk_",
      "packages": [
        "guzzlehttp/guzzle"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "\"vendor/bin/mozart\" compose",
      "composer dump-autoload"
    ],
    "post-update-cmd": [
      "\"vendor/bin/mozart\" compose",
      "composer dump-autoload"
    ]
  }
}